module samsung_command (	input logic clk,
									input logic [3:0] KEY,
									output logic [31:0] command);
									
	//assume is initialized to 0
	logic pressed3;
	logic [31:0] counter3;

	
	//key3 is power
	always_ff @(posedge clk)
	begin
		//key3 and debouncing
		if (~KEY[3])
		begin
			if (~pressed3)
			begin
				command <= 32'he0e09966;										//-------------------------command goes here--------------------------//
				pressed3 <= ~pressed3;
			end
		end

		if (pressed3)
		begin
			if (counter3 == 32'd20_000_000)
			begin
				//can register another press
				counter3 <= 0;
				pressed3 <= ~pressed3;
			end
			else
				counter3 <= counter3 + 1;
		end
	end		//end always @ff
endmodule
									